<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.pojo.Estudiante" table="Estudiante" schema="taller1" lazy="false">
		<id name="ci" column="Ci" type="integer">
			<generator class="assigned">
			</generator>
		</id>
		<property name="nombre" column="Nombre" type="string" length="255" not-null="false" lazy="false"/>
		<property name="ap" column="Ap" type="string" length="255" not-null="false" lazy="false"/>
		<property name="am" column="Am" type="string" length="255" not-null="false" lazy="false"/>
		<property name="correo" column="Correo" type="string" length="255" not-null="false" lazy="false"/>
		<property name="telefono" column="Telefono" type="string" length="255" not-null="false" lazy="false"/>
		<property name="usuario" column="Usuario" type="string" length="255" not-null="false" lazy="false"/>
		<property name="contrasena" column="Contrasena" type="string" length="255" not-null="false" lazy="false"/>
		<property name="direccion" column="Direccion" type="string" length="255" not-null="false" lazy="false"/>
		<property name="imagen" column="Imagen" type="string" length="255" not-null="false" lazy="false"/>
		<property name="estado" column="Estado" type="boolean" not-null="true" lazy="false"/>
		<property name="rude" column="Rude" unique="true" type="integer" not-null="true" lazy="false"/>
		<set name="notasests" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="Estudianteci" not-null="true"/>
			<one-to-many class="model.pojo.Notasest"/>
		</set>
		<set name="matriculas" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="Estudianteci" not-null="true"/>
			<one-to-many class="model.pojo.Matricula"/>
		</set>
	</class>
</hibernate-mapping>
