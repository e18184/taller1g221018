<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.pojo.Matricula" table="Matricula" schema="taller1" lazy="false">
		<composite-id>
			<key-many-to-one name="estudiante" column="Estudianteci" class="model.pojo.Estudiante">
			</key-many-to-one>
			<key-many-to-one name="paralelo" column="Idparalelo" class="model.pojo.Paralelo">
			</key-many-to-one>
			<key-property name="fecha" column="Fecha" type="date"/>
		</composite-id>
		<property name="estado" column="Estado" type="boolean" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
