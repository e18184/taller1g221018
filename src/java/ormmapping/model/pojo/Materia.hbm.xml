<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.pojo.Materia" table="Materia" schema="taller1" lazy="false">
		<id name="sigla" column="Sigla" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<many-to-one name="curso" cascade="lock" column="CursoID" class="model.pojo.Curso" not-null="true" lazy="proxy">
		</many-to-one>
		<property name="nombre" column="Nombre" type="string" length="255" not-null="false" lazy="false"/>
		<property name="progama" column="Progama" type="string" length="255" not-null="false" lazy="false"/>
		<property name="nrohora" column="Nrohora" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="estado" column="Estado" type="boolean" not-null="true" lazy="false"/>
		<set name="designaEspecialidads" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="MateriaID" not-null="true"/>
			<one-to-many class="model.pojo.DesignaEspecialidad"/>
		</set>
		<set name="paralelos" table="Materia_Paralelo" schema="taller1" lazy="true" cascade="save-update,lock">
			<key column="MateriaSigla" not-null="true"/>
			<many-to-many column="Paraleloidparalelo" class="model.pojo.Paralelo">
			</many-to-many>
		</set>
		<set name="notasests" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="MateriaSigla" not-null="true"/>
			<one-to-many class="model.pojo.Notasest"/>
		</set>
	</class>
</hibernate-mapping>
