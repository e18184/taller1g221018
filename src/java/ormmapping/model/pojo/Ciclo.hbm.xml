<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.pojo.Ciclo" table="Ciclo" schema="taller1" lazy="false">
		<id name="idciclo" column="idciclo" type="integer">
			<generator class="assigned">
			</generator>
		</id>
		<many-to-one name="director" column="DirectorID" class="model.pojo.Director" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy">
		</many-to-one>
		<property name="nombre" column="Nombre" type="string" length="255" not-null="false" lazy="false"/>
		<property name="turno" column="Turno" type="string" length="255" not-null="false" lazy="false"/>
		<property name="estado" column="Estado" type="boolean" not-null="true" lazy="false"/>
		<set name="curso" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="CicloID" not-null="true"/>
			<one-to-many class="model.pojo.Curso"/>
		</set>
	</class>
</hibernate-mapping>
